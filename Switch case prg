import java.util.Scanner;
public class Switch {
public statis void main(String[] args){
int input;
System.out.println("Pick one : 1. Hi\t 2. Hello\t 3. Hey\t");
Scanner sc = new Scanner(System.in);
input = sc.nextInt();
switch(input)
{
case 1: System.out.println("Hi, Iam good. ");
break;
case 2: System.out.printn("Hello, Iam good. ");
break;
case 3: System.out.println("Hey, Iam good. ");
break;
default: System.out.println("Invalid input ");
}
}
}
